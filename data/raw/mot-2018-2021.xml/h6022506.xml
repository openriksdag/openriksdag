							&lt;span&gt;Fiskarheden&lt;/span&gt;
						&lt;/p&gt;
					&lt;/td&gt;
					&lt;td style="padding-left:3.4pt; padding-right:3.4pt; vertical-align:top; width:135pt"&gt;
						&lt;p style="margin-top:4pt; margin-bottom:4pt; t-i:0pt; line-height:15pt"&gt;
