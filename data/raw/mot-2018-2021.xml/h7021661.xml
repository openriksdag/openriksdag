			&lt;/table&gt;
			&lt;p&gt;
				&lt;span&gt; &lt;/span&gt;
			&lt;/p&gt;
		&lt;/div&gt;
		&lt;hr style="width:33%; height:1px; text-align:left; -aw-footnote-numberstyle:0; -aw-footnote-startnumber:1; -aw-footnote-type:0" /&gt;
		&lt;div id="_ftn1" style="-aw-footnote-isauto:1"&gt;
			&lt;p class="FootnoteText"&gt;
				&lt;a href="#_ftnref1"&gt;&lt;span class="FootnoteReference" style="font-size:8pt"&gt;[1]&lt;/span&gt;&lt;/a&gt;&lt;span&gt; &lt;/span&gt;&lt;span&gt;Schlytter&lt;/span&gt;&lt;span&gt;, A., &lt;/span&gt;&lt;span&gt;Högdin&lt;/span&gt;&lt;span&gt;, S., &lt;/span&gt;&lt;span&gt;Ghadimi&lt;/span&gt;&lt;span&gt;, M., Backlund, Å. &amp; &lt;/span&gt;&lt;span&gt;Rexvid&lt;/span&gt;&lt;span&gt;, D. år 2009.&lt;/span&gt;
			&lt;/p&gt;
		&lt;/div&gt;
